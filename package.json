{
  "name": "chord-renderer",
  "version": "0.0.1",
  "description": "A JavaScript library for working with guitar chords, voicings, and inversions",
  "main": "lib/chord.js",
  "types": "types/chord.d.ts",
  "type": "module",
  "scripts": {
    "test": "node --test test/*.test.js",
    "test:watch": "node --test --watch test/*.test.js",
    "build:types": "tsc",
    "build": "node ./scripts/build.js",
    "serve": "npx http-server -p 8080 docs"
  },
  "keywords": [
    "guitar",
    "chords",
    "music",
    "voicing",
    "intervals",
    "inversions"
  ],
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sithmel/chord-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/sithmel/chord-renderer/issues"
  },
  "homepage": "https://github.com/sithmel/chord-renderer#readme",
  "devDependencies": {
    "esbuild": "^0.27.2",
    "jsdom": "^27.4.0",
    "svguitar": "^2.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "text-guitar-chart": "^0.1.2"
  }
}
